{
  "name": "egg-mongodb",
  "version": "1.0.8",
  "description": "A mongodb adapter for egg.js",
  "eggPlugin": {
    "name": "mongodb"
  },
  "dependencies": {
    "mongodb": "^3.0.5"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "egg-bin": "^4.4.1",
    "egg-ci": "^1.7.0",
    "eslint": "^4.0.0",
    "eslint-config-egg": "^7.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint .",
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "pkgfiles": "egg-bin pkgfiles"
  },
  "ci": {
    "version": "6, 8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xxmyjk/egg-mongodb.git"
  },
  "homepage": "https://github.com/xxmyjk/egg-mongodb#README",
  "author": "Jin Yi",
  "license": "MIT"
}
